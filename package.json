{
  "name": "tiptap-markdown-editor",
  "displayName": "Tiptap Markdown Editor",
  "description": "WYSIWYG Markdown editor for VS Code using Tiptap",
  "version": "0.0.1",
  "publisher": "Kuba Serafinowski",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "tiptapMarkdown.editor",
        "displayName": "Tiptap Markdown Editor",
        "selector": [
          {
            "filenamePattern": "*.md"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run build",
    "build": "pnpm run build:extension && pnpm run build:webview",
    "build:extension": "vite build",
    "build:webview": "cd webview && pnpm run build",
    "watch": "vite build --watch",
    "dev": "pnpm run watch"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}